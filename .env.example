# OnchainKit API Key (required for Base Mini Apps)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key_here

# OpenAI API Key (required for legal advice generation)
OPENAI_API_KEY=your_openai_api_key_here

# App URL (for production deployment)
NEXT_PUBLIC_URL=https://your-app-domain.com

# Optional: Redis URL for caching (if using Upstash)
UPSTASH_REDIS_REST_URL=your_redis_url_here
UPSTASH_REDIS_REST_TOKEN=your_redis_token_here
